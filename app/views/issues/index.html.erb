<div class="offset-1 col-10">
  <%= render 'issues/search_form', team: @team %>

  <% @pagenated_issues.each do |issue| %>
    <div class="row mt-3">
      <%= render 'issues/index_card', team: @team, issue: issue %>
    </div>
  <% end %>
</div>

<div class="row justify-content-center my-5">
  <%= paginate @pagenated_issues %>
</div>
