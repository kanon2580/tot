<div class="row flex-column align-items-center">
  <h3 class="headline">Teams</h3>
</div>

<% team_members.each do |team_member| %>
  <div class="row mt-3 flex-column align-items-center">
    <%= link_to team_member.team.name, team_path(team_member.team), class: "btn btn-sm btn-dark"%>
  </div>
<% end %>