<h1>Members</h1>

<% @team_members.each do |team_member| %>
  <%= link_to team_user_path(id: team_member.user.id) do %>
    <%= attachment_image_tag team_member.user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "noimage.jpg", size: "100x100" %>
    <%= team_member.user.name %>
  <% end %>
<% end %>